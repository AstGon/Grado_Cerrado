--
-- TOC entry 245 (class 1259 OID 28201)
-- Name: vista_rendimiento_subtemas_completo; Type: VIEW; Schema: public; Owner: adminuser
--

CREATE VIEW public.vista_rendimiento_subtemas_completo AS
 SELECT id,
    estudiante_id,
    subtema_id,
    tema_id,
    total_intentos,
    total_correctas,
    total_incorrectas,
    tasa_acierto,
    ultima_practica,
    fecha_primer_intento,
    fecha_actualizacion,
    racha_aciertos_consecutivos,
    tiempo_promedio_respuesta,
    public.calcular_nivel_dominio(tasa_acierto, total_intentos, racha_aciertos_consecutivos, EXTRACT(day FROM (CURRENT_TIMESTAMP - (ultima_practica)::timestamp with time zone))) AS nivel_dominio,
    EXTRACT(day FROM (CURRENT_TIMESTAMP - (ultima_practica)::timestamp with time zone)) AS dias_sin_practicar
   FROM public.estudiante_rendimiento_subtemas ers;


ALTER VIEW public.vista_rendimiento_subtemas_completo OWNER TO adminuser;

--
-- TOC entry 247 (class 1259 OID 28210)
-- Name: vista_debilidades_subtemas; Type: VIEW; Schema: public; Owner: adminuser
--

CREATE VIEW public.vista_debilidades_subtemas AS
 SELECT vrs.estudiante_id,
    e.nombre_completo,
    a.nombre AS area,
    t.nombre AS tema,
    s.nombre AS subtema,
    vrs.total_intentos,
    vrs.total_correctas,
    vrs.total_incorrectas,
    vrs.tasa_acierto,
    vrs.nivel_dominio,
    vrs.dias_sin_practicar,
    vrs.racha_aciertos_consecutivos,
    vrs.tiempo_promedio_respuesta,
    vrs.fecha_primer_intento,
    vrs.ultima_practica
   FROM ((((public.vista_rendimiento_subtemas_completo vrs
     JOIN public.estudiantes e ON ((vrs.estudiante_id = e.id)))
     JOIN public.subtemas s ON ((vrs.subtema_id = s.id)))
     JOIN public.temas t ON ((vrs.tema_id = t.id)))
     JOIN public.areas a ON ((t.area_id = a.id)))
  WHERE (vrs.total_intentos >= 2)
  ORDER BY vrs.estudiante_id, vrs.tasa_acierto;


ALTER VIEW public.vista_debilidades_subtemas OWNER TO adminuser;

--
-- TOC entry 244 (class 1259 OID 28197)
-- Name: vista_rendimiento_temas_completo; Type: VIEW; Schema: public; Owner: adminuser
--

CREATE VIEW public.vista_rendimiento_temas_completo AS
 SELECT id,
    estudiante_id,
    tema_id,
    total_intentos,
    total_correctas,
    total_incorrectas,
    tasa_acierto,
    ultima_practica,
    fecha_primer_intento,
    fecha_actualizacion,
    racha_aciertos_consecutivos,
    racha_aciertos_maxima,
    tiempo_promedio_respuesta,
    public.calcular_nivel_dominio(tasa_acierto, total_intentos, racha_aciertos_consecutivos, EXTRACT(day FROM (CURRENT_TIMESTAMP - (ultima_practica)::timestamp with time zone))) AS nivel_dominio,
    EXTRACT(day FROM (CURRENT_TIMESTAMP - (ultima_practica)::timestamp with time zone)) AS dias_sin_practicar
   FROM public.estudiante_rendimiento_temas ert;


ALTER VIEW public.vista_rendimiento_temas_completo OWNER TO adminuser;

--
-- TOC entry 4490 (class 0 OID 0)
-- Dependencies: 244
-- Name: VIEW vista_rendimiento_temas_completo; Type: COMMENT; Schema: public; Owner: adminuser
--

COMMENT ON VIEW public.vista_rendimiento_temas_completo IS 'Vista con nivel_dominio calculado dinámicamente. Usar esta vista en lugar de la tabla directamente.';


--
-- TOC entry 246 (class 1259 OID 28205)
-- Name: vista_debilidades_temas; Type: VIEW; Schema: public; Owner: adminuser
--

CREATE VIEW public.vista_debilidades_temas AS
 SELECT vrt.estudiante_id,
    e.nombre_completo,
    a.nombre AS area,
    t.nombre AS tema,
    vrt.total_intentos,
    vrt.total_correctas,
    vrt.total_incorrectas,
    vrt.tasa_acierto,
    vrt.nivel_dominio,
    vrt.dias_sin_practicar,
    vrt.racha_aciertos_consecutivos,
    vrt.racha_aciertos_maxima,
    vrt.tiempo_promedio_respuesta,
    vrt.fecha_primer_intento,
    vrt.ultima_practica
   FROM (((public.vista_rendimiento_temas_completo vrt
     JOIN public.estudiantes e ON ((vrt.estudiante_id = e.id)))
     JOIN public.temas t ON ((vrt.tema_id = t.id)))
     JOIN public.areas a ON ((t.area_id = a.id)))
  WHERE (vrt.total_intentos >= 3)
  ORDER BY vrt.estudiante_id, vrt.tasa_acierto;


ALTER VIEW public.vista_debilidades_temas OWNER TO adminuser;

--
-- TOC entry 4491 (class 0 OID 0)
-- Dependencies: 246
