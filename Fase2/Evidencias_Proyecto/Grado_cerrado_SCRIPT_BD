--
-- TOC entry 283 (class 1255 OID 28326)
-- Name: obtener_fechas_estudio_semana(integer, integer); Type: FUNCTION; Schema: public; Owner: adminuser
--

CREATE FUNCTION public.obtener_fechas_estudio_semana(p_estudiante_id integer, p_semanas_atras integer DEFAULT 1) RETURNS TABLE(fecha date, cantidad_tests bigint, total_preguntas bigint, promedio_aciertos numeric)
    LANGUAGE plpgsql
    AS $$
BEGIN
    RETURN QUERY
    SELECT 
        t.fecha_test as fecha,
        COUNT(DISTINCT t.id) as cantidad_tests,
        COUNT(tp.id) as total_preguntas,
        ROUND(
            COALESCE(
                AVG(CASE WHEN tp.es_correcta THEN 100.0 ELSE 0.0 END),
                0
            ),
            2
        ) as promedio_aciertos
    FROM tests t
    LEFT JOIN test_preguntas tp ON t.id = tp.test_id
    WHERE t.estudiante_id = p_estudiante_id
      AND t.fecha_test >= CURRENT_DATE - (p_semanas_atras || ' weeks')::INTERVAL
      AND t.estado IN ('en_proceso', 'dominado')
    GROUP BY t.fecha_test
    ORDER BY t.fecha_test DESC;
END;
$$;


ALTER FUNCTION public.obtener_fechas_estudio_semana(p_estudiante_id integer, p_semanas_atras integer) OWNER TO adminuser;

