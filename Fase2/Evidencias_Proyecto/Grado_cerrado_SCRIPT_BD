--

COMMENT ON FUNCTION public.calcular_nivel_dominio(p_tasa_acierto numeric, p_total_intentos integer, p_racha_actual integer, p_dias_sin_practicar numeric) IS 'Calcula nivel de dominio basado en 4 factores: tasa (40%), intentos (20%), racha (20%), tiempo (20%)';


--
-- TOC entry 284 (class 1255 OID 28327)
-- Name: calcular_racha_cumplimiento(integer); Type: FUNCTION; Schema: public; Owner: adminuser
--

CREATE FUNCTION public.calcular_racha_cumplimiento(p_estudiante_id integer) RETURNS TABLE(racha_actual integer, racha_maxima integer, semanas_consecutivas integer)
    LANGUAGE plpgsql
    AS $$
DECLARE
    v_objetivo INTEGER;
    v_racha_actual INTEGER := 0;
    v_racha_maxima INTEGER := 0;
    v_fecha DATE;
BEGIN
    SELECT frecuencia_estudio_semanal INTO v_objetivo
    FROM estudiantes
    WHERE id = p_estudiante_id;
    
    FOR v_fecha IN 
        SELECT DATE_TRUNC('week', fecha_test)::DATE as semana
        FROM tests
        WHERE estudiante_id = p_estudiante_id
          AND estado IN ('en_proceso', 'dominado')
        GROUP BY DATE_TRUNC('week', fecha_test)
        HAVING COUNT(DISTINCT fecha_test) >= v_objetivo
        ORDER BY DATE_TRUNC('week', fecha_test) DESC
    LOOP
        v_racha_actual := v_racha_actual + 1;
        IF v_racha_actual > v_racha_maxima THEN
            v_racha_maxima := v_racha_actual;
        END IF;
    END LOOP;
    
    RETURN QUERY SELECT v_racha_actual, v_racha_maxima, v_racha_actual;
END;
$$;


ALTER FUNCTION public.calcular_racha_cumplimiento(p_estudiante_id integer) OWNER TO adminuser;

--
-- TOC entry 255 (class 1255 OID 28325)
-- Name: estudiante_estudio_hoy(integer); Type: FUNCTION; Schema: public; Owner: adminuser
--

CREATE FUNCTION public.estudiante_estudio_hoy(p_estudiante_id integer) RETURNS boolean
    LANGUAGE plpgsql
    AS $$
BEGIN
    RETURN EXISTS (
        SELECT 1 
        FROM tests 
        WHERE estudiante_id = p_estudiante_id 
          AND fecha_test = CURRENT_DATE
          AND estado IN ('en_proceso', 'dominado')
    );
END;
$$;


ALTER FUNCTION public.estudiante_estudio_hoy(p_estudiante_id integer) OWNER TO adminuser;

