--
-- TOC entry 256 (class 1255 OID 28331)
-- Name: update_tasa_acierto(); Type: FUNCTION; Schema: public; Owner: adminuser
--

CREATE FUNCTION public.update_tasa_acierto() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
    IF NEW.veces_utilizada > 0 THEN
        NEW.tasa_acierto = (NEW.veces_correcta::DECIMAL / NEW.veces_utilizada) * 100;
    ELSE
        NEW.tasa_acierto = NULL;
    END IF;
    RETURN NEW;
END;
$$;


ALTER FUNCTION public.update_tasa_acierto() OWNER TO adminuser;

--
-- TOC entry 254 (class 1255 OID 25125)
-- Name: uuid_via_md5(); Type: FUNCTION; Schema: public; Owner: adminuser
--

CREATE FUNCTION public.uuid_via_md5() RETURNS uuid
    LANGUAGE sql
    AS $$
  SELECT (
    substr(hex,1,8) || '-' ||
    substr(hex,9,4) || '-' ||
    substr(hex,13,4) || '-' ||
    substr(hex,17,4) || '-' ||
    substr(hex,21,12)
  )::uuid
  FROM (SELECT md5(random()::text || clock_timestamp()::text) AS hex) s;
$$;


ALTER FUNCTION public.uuid_via_md5() OWNER TO adminuser;

