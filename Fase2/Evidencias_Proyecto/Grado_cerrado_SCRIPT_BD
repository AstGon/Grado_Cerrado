-- TOC entry 226 (class 1259 OID 27655)
-- Name: estudiantes; Type: TABLE; Schema: public; Owner: adminuser
--

CREATE TABLE public.estudiantes (
    id integer NOT NULL,
    nombre character varying(100) NOT NULL,
    segundo_nombre character varying(100),
    apellido_paterno character varying(100),
    apellido_materno character varying(100),
    nombre_completo character varying(200) GENERATED ALWAYS AS (((((COALESCE(nombre, ''::character varying))::text ||
CASE
    WHEN ((segundo_nombre IS NOT NULL) AND ((segundo_nombre)::text <> ''::text)) THEN (' '::text || (segundo_nombre)::text)
    ELSE ''::text
END) ||
CASE
    WHEN ((apellido_paterno IS NOT NULL) AND ((apellido_paterno)::text <> ''::text)) THEN (' '::text || (apellido_paterno)::text)
    ELSE ''::text
END) ||
CASE
    WHEN ((apellido_materno IS NOT NULL) AND ((apellido_materno)::text <> ''::text)) THEN (' '::text || (apellido_materno)::text)
    ELSE ''::text
END)) STORED,
    email character varying(100) NOT NULL,
    password_hash character varying(255) NOT NULL,
    foto_perfil character varying(255),
    id_avatar_seleccionado character varying(50),
    nivel_actual public.nivel_dificultad DEFAULT 'basico'::public.nivel_dificultad,
    nivel_diagnosticado public.nivel_dificultad,
    test_diagnostico_completado boolean DEFAULT false,
    fecha_test_diagnostico timestamp without time zone,
    fecha_registro timestamp without time zone DEFAULT CURRENT_TIMESTAMP,
    ultimo_acceso timestamp without time zone DEFAULT CURRENT_TIMESTAMP,
    activo boolean DEFAULT true,
    verificado boolean DEFAULT false,
    frecuencia_estudio_semanal integer DEFAULT 3,
    objetivo_dias_estudio character varying(50) DEFAULT 'flexible'::character varying,
    dias_preferidos_estudio jsonb DEFAULT '[]'::jsonb,
    recordatorio_estudio_activo boolean DEFAULT true,
