-- TOC entry 281 (class 1255 OID 28196)
-- Name: calcular_nivel_dominio(numeric, integer, integer, numeric); Type: FUNCTION; Schema: public; Owner: adminuser
--

CREATE FUNCTION public.calcular_nivel_dominio(p_tasa_acierto numeric, p_total_intentos integer, p_racha_actual integer, p_dias_sin_practicar numeric) RETURNS character varying
    LANGUAGE plpgsql
    AS $$
DECLARE
    v_puntos DECIMAL := 0;
BEGIN
    -- Tasa de acierto (40%)
    v_puntos := v_puntos + CASE
        WHEN p_tasa_acierto >= 90 THEN 40
        WHEN p_tasa_acierto >= 75 THEN 30
        WHEN p_tasa_acierto >= 60 THEN 20
        WHEN p_tasa_acierto >= 40 THEN 10
        ELSE 0
    END;

    -- Intentos (20%)
    v_puntos := v_puntos + CASE
        WHEN p_total_intentos >= 20 THEN 20
        WHEN p_total_intentos >= 10 THEN 15
        WHEN p_total_intentos >= 5 THEN 10
        ELSE 5
    END;

    -- Racha (20%)
    v_puntos := v_puntos + CASE
        WHEN p_racha_actual >= 5 THEN 20
        WHEN p_racha_actual >= 3 THEN 15
        WHEN p_racha_actual >= 1 THEN 10
        ELSE 0
    END;

    -- Tiempo (20%)
    v_puntos := v_puntos + CASE
        WHEN p_dias_sin_practicar IS NULL THEN 10
        WHEN p_dias_sin_practicar <= 3 THEN 20
        WHEN p_dias_sin_practicar <= 7 THEN 15
        WHEN p_dias_sin_practicar <= 14 THEN 10
        WHEN p_dias_sin_practicar <= 30 THEN 5
        ELSE 0
    END;

    RETURN CASE
        WHEN v_puntos >= 80 THEN 'excelente'
        WHEN v_puntos >= 60 THEN 'bueno'
        WHEN v_puntos >= 40 THEN 'medio'
        WHEN v_puntos >= 20 THEN 'debil'
        ELSE 'critico'
    END;
END;
$$;


ALTER FUNCTION public.calcular_nivel_dominio(p_tasa_acierto numeric, p_total_intentos integer, p_racha_actual integer, p_dias_sin_practicar numeric) OWNER TO adminuser;

