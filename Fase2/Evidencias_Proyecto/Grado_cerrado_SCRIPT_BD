--
-- TOC entry 243 (class 1259 OID 28166)
-- Name: estudiante_rendimiento_subtemas; Type: TABLE; Schema: public; Owner: adminuser
--

CREATE TABLE public.estudiante_rendimiento_subtemas (
    id integer NOT NULL,
    estudiante_id integer NOT NULL,
    subtema_id integer NOT NULL,
    tema_id integer NOT NULL,
    total_intentos integer DEFAULT 0,
    total_correctas integer DEFAULT 0,
    total_incorrectas integer DEFAULT 0,
    tasa_acierto numeric(5,2) GENERATED ALWAYS AS (
CASE
    WHEN (total_intentos > 0) THEN round((((total_correctas)::numeric / (total_intentos)::numeric) * (100)::numeric), 2)
    ELSE (0)::numeric
END) STORED,
    ultima_practica timestamp without time zone,
    fecha_primer_intento timestamp without time zone DEFAULT CURRENT_TIMESTAMP,
    fecha_actualizacion timestamp without time zone DEFAULT CURRENT_TIMESTAMP,
    racha_aciertos_consecutivos integer DEFAULT 0,
    tiempo_promedio_respuesta integer
);


ALTER TABLE public.estudiante_rendimiento_subtemas OWNER TO adminuser;

