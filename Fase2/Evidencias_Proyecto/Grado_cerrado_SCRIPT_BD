--
-- TOC entry 282 (class 1255 OID 28215)
-- Name: actualizar_rendimiento_tema_completo(); Type: FUNCTION; Schema: public; Owner: adminuser
--

CREATE FUNCTION public.actualizar_rendimiento_tema_completo() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
DECLARE
    v_tema_id INT;
    v_subtema_id INT;
    v_es_correcta BOOLEAN;
    v_estudiante_id INT;
    v_tiempo_respuesta INT;
BEGIN
    -- Obtener datos de la pregunta
    SELECT pg.tema_id, pg.subtema_id INTO v_tema_id, v_subtema_id
    FROM preguntas_generadas pg
    WHERE pg.id = NEW.pregunta_generada_id;
    
    -- Obtener estudiante
    SELECT estudiante_id INTO v_estudiante_id
    FROM tests
    WHERE id = NEW.test_id;
    
    v_es_correcta := NEW.es_correcta;
    v_tiempo_respuesta := NEW.tiempo_respuesta_segundos;
    
    -- ═══════════════════════════════════════════════════════
    -- ACTUALIZAR RENDIMIENTO POR TEMA
    -- ═══════════════════════════════════════════════════════
    INSERT INTO estudiante_rendimiento_temas (
        estudiante_id, tema_id, total_intentos, total_correctas, total_incorrectas,
        ultima_practica, racha_aciertos_consecutivos, tiempo_promedio_respuesta
    )
    VALUES (
        v_estudiante_id, v_tema_id, 1,
        CASE WHEN v_es_correcta THEN 1 ELSE 0 END,
        CASE WHEN NOT v_es_correcta THEN 1 ELSE 0 END,
        NEW.fecha_respuesta,
        CASE WHEN v_es_correcta THEN 1 ELSE 0 END,
        v_tiempo_respuesta
    )
    ON CONFLICT (estudiante_id, tema_id) 
    DO UPDATE SET
        total_intentos = estudiante_rendimiento_temas.total_intentos + 1,
        total_correctas = estudiante_rendimiento_temas.total_correctas + 
            CASE WHEN v_es_correcta THEN 1 ELSE 0 END,
        total_incorrectas = estudiante_rendimiento_temas.total_incorrectas + 
            CASE WHEN NOT v_es_correcta THEN 1 ELSE 0 END,
        racha_aciertos_consecutivos = 
            CASE WHEN v_es_correcta 
            THEN estudiante_rendimiento_temas.racha_aciertos_consecutivos + 1
            ELSE 0 END,
        racha_aciertos_maxima = GREATEST(
            estudiante_rendimiento_temas.racha_aciertos_maxima,
            CASE WHEN v_es_correcta 
            THEN estudiante_rendimiento_temas.racha_aciertos_consecutivos + 1
            ELSE 0 END
        ),
        tiempo_promedio_respuesta = 
            CASE WHEN v_tiempo_respuesta IS NOT NULL 
            THEN (COALESCE(estudiante_rendimiento_temas.tiempo_promedio_respuesta, 0) * 
                  estudiante_rendimiento_temas.total_intentos + v_tiempo_respuesta) / 
                 (estudiante_rendimiento_temas.total_intentos + 1)
            ELSE estudiante_rendimiento_temas.tiempo_promedio_respuesta END,
        ultima_practica = NEW.fecha_respuesta,
        fecha_actualizacion = CURRENT_TIMESTAMP;
    
    -- ═══════════════════════════════════════════════════════
    -- ACTUALIZAR RENDIMIENTO POR SUBTEMA (si existe)
    -- ═══════════════════════════════════════════════════════
    IF v_subtema_id IS NOT NULL THEN
        INSERT INTO estudiante_rendimiento_subtemas (
            estudiante_id, subtema_id, tema_id, total_intentos, total_correctas, 
            total_incorrectas, ultima_practica, racha_aciertos_consecutivos, 
            tiempo_promedio_respuesta
        )
        VALUES (
            v_estudiante_id, v_subtema_id, v_tema_id, 1,
            CASE WHEN v_es_correcta THEN 1 ELSE 0 END,
            CASE WHEN NOT v_es_correcta THEN 1 ELSE 0 END,
            NEW.fecha_respuesta,
            CASE WHEN v_es_correcta THEN 1 ELSE 0 END,
            v_tiempo_respuesta
        )
        ON CONFLICT (estudiante_id, subtema_id) 
        DO UPDATE SET
            total_intentos = estudiante_rendimiento_subtemas.total_intentos + 1,
            total_correctas = estudiante_rendimiento_subtemas.total_correctas + 
                CASE WHEN v_es_correcta THEN 1 ELSE 0 END,
            total_incorrectas = estudiante_rendimiento_subtemas.total_incorrectas + 
                CASE WHEN NOT v_es_correcta THEN 1 ELSE 0 END,
            racha_aciertos_consecutivos = 
                CASE WHEN v_es_correcta 
                THEN estudiante_rendimiento_subtemas.racha_aciertos_consecutivos + 1
                ELSE 0 END,
            tiempo_promedio_respuesta = 
                CASE WHEN v_tiempo_respuesta IS NOT NULL 
                THEN (COALESCE(estudiante_rendimiento_subtemas.tiempo_promedio_respuesta, 0) * 
                      estudiante_rendimiento_subtemas.total_intentos + v_tiempo_respuesta) / 
                     (estudiante_rendimiento_subtemas.total_intentos + 1)
                ELSE estudiante_rendimiento_subtemas.tiempo_promedio_respuesta END,
            ultima_practica = NEW.fecha_respuesta,
            fecha_actualizacion = CURRENT_TIMESTAMP;
    END IF;
    
    RETURN NEW;
END;
$$;


ALTER FUNCTION public.actualizar_rendimiento_tema_completo() OWNER TO adminuser;

