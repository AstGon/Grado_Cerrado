--
-- TOC entry 230 (class 1259 OID 27793)
-- Name: preguntas_generadas; Type: TABLE; Schema: public; Owner: adminuser
--

CREATE TABLE public.preguntas_generadas (
    id integer NOT NULL,
    tipo public.tipo_pregunta NOT NULL,
    modalidad_id integer NOT NULL,
    tema_id integer NOT NULL,
    nivel public.nivel_dificultad NOT NULL,
    texto_pregunta text NOT NULL,
    respuesta_correcta_boolean boolean,
    respuesta_correcta_opcion character(1),
    respuesta_modelo text,
    explicacion text,
    activa boolean DEFAULT true,
    creada_por character varying(100) DEFAULT 'sistema'::character varying,
    fecha_creacion timestamp without time zone DEFAULT CURRENT_TIMESTAMP,
    fecha_actualizacion timestamp without time zone DEFAULT CURRENT_TIMESTAMP,
    veces_utilizada integer DEFAULT 0,
    veces_correcta integer DEFAULT 0,
    ultimo_uso timestamp without time zone,
    tasa_acierto numeric(4,3) GENERATED ALWAYS AS (
CASE
    WHEN (veces_utilizada > 0) THEN round(((veces_correcta)::numeric / (veces_utilizada)::numeric), 3)
    ELSE (0)::numeric
END) STORED,
    prompt_sistema_id integer,
    contexto_fragmentos text,
    modelo_ia character varying(100) DEFAULT 'gpt-4'::character varying,
    calidad_estimada numeric(3,2) DEFAULT 0.8,
    subtema_id integer,
    source_chunks text[],
    CONSTRAINT check_modalidad_tipo CHECK ((((modalidad_id = 1) AND (tipo = ANY (ARRAY['verdadero_falso'::public.tipo_pregunta, 'seleccion_multiple'::public.tipo_pregunta]))) OR ((modalidad_id = 2) AND (tipo = 'desarrollo'::public.tipo_pregunta)))),
    CONSTRAINT preguntas_generadas_respuesta_correcta_opcion_check CHECK ((respuesta_correcta_opcion = ANY (ARRAY['A'::bpchar, 'B'::bpchar, 'C'::bpchar, 'D'::bpchar])))
);


ALTER TABLE public.preguntas_generadas OWNER TO adminuser;

